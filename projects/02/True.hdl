/**
 * True gate: 
 * out = 1
 */

CHIP True {
    IN in;
    OUT out;

    PARTS:
    Xor(a=in,b=in,out=loki); //Gera um valor zero, pois as entradas v√£o se iguais
    Not(in=loki,out=out); //Negua a entrada zero, a transformando em 1
}
